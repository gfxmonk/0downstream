	<implementation arch="*-src" compile:dup-src="true" id="sha1new=a721f76db885bc1ca81421fe558a52546cd2ce00" version="1.1.8">
		<requires interface="http://gfxmonk.github.io/0downstream/feeds/opam/ocamlfind.xml" source="true"/>
		<requires interface="http://gfxmonk.github.io/0downstream/feeds/opam/cppo.xml" source="true"/>
		<requires interface="http://gfxmonk.github.io/0downstream/feeds/opam/easy-format.xml" source="true"/>
		<requires interface="http://gfxmonk.github.io/0downstream/feeds/opam/biniou.xml" source="true"/>
		<environment insert="opam" mode="prepend" name="OPAM_PKG_PATH"/>
		<requires interface="http://gfxmonk.net/dist/0install/ocaml.xml"/>
		<command name="compile">
			<runner interface="http://gfxmonk.github.io/0downstream/files/opam-src-build.xml"/>
			<requires interface="http://gfxmonk.github.io/0downstream/files/opam-src-build.xml">
				<version not-before="0.1.1"/>
			</requires>
			<arg>yojson</arg>
			<compile:implementation>
				<environment insert="bin" mode="prepend" name="PATH"/>
				<command name="run" path="bin/ydump"/>
			</compile:implementation>
		</command>
		<recipe>
			<archive href="http://mjambon.com/releases/yojson/yojson-1.1.8.tar.gz" size="25157"/>
			<rename dest="src" source="yojson-1.1.8"/>
